name: Create Cache


jobs:
  create_cache:
    runs-on: ubuntu-latest
    steps:
      - name: Cache Salto Deployment ID
        uses: actions/cache@v3
        with:
          key: salto-${{github.event.ref}}
          path: |
            ~/salto-*
      - name: create a branch file
        run: |
          echo ${{github.ref}}
          echo ${{github.event.ref}}
          touch ~/salto-${{github.event.ref}}

  
